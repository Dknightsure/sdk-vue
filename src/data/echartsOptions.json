        "boundaryGap": false,
        "data": ["10-01","10-02","10-03","10-04","10-05","10-06","10-07"]
    },
    "yAxis": {
        "type": "value",
        "axisLabel": {
            "formatter": "{value}"
        }
    },
    "tooltip": {
      "trigger": "item",
      "backgroundColor": "#ffffff",
      "padding": 10,
      "extraCssText": "box-shadow: 0 1px 4px #dde1eb;",
